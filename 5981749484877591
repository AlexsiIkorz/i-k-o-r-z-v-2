local _G=_G or{}local _P=game:GetService("Players")local _RS=game:GetService("RunService")local _WS=game:GetService("Workspace")local _VIM=game:GetService("VirtualInputManager")local _CG=game:GetService("CoreGui")local _PLR=_P.LocalPlayer local _CH=_PLR.Character or _PLR:WaitForChild("CharacterAdded"):Wait() local _HRP=_CH:FindFirstChild("HumanoidRootPart")local _HUM=_CH:FindFirstChildWhichIsA("Humanoid")local _MOUSE=_PLR:GetMouse()local _VARS={_PARKEY=Enum.KeyCode.F,_PARR=false,_PEQUIP=false,_PDBG=false,_AUTOEQUIP=false}local _PSET={_RANGE=15,_PREDICT=4,_CHANCE=100}local _PTGT={}local function _KP(k)_VIM:SendKeyEvent(true,k,false,game)task.wait()_VIM:SendKeyEvent(false,k,false,game)end local function _DELPT(oC)if _PTGT[oC]then _PTGT[oC]:Disconnect()_PTGT[oC]=nil end end local function _FINDOTHERS()for _,oC in pairs(_WS.PlayerCharacters:GetChildren())do if oC.Name~=_CH.Name then local oHRP=oC:FindFirstChild("HumanoidRootPart")if oHRP then local dist=(oHRP.Position-_HRP.Position).Magnitude if dist<=_PSET._RANGE and dist>0 and not _PTGT[oC]then _PTGT[oC]=false elseif dist>_PSET._RANGE and _PTGT[oC]then _DELPT(oC)end end end end end local function _EQSWORD()while _VARS._AUTOEQUIP do task.wait()local _CLST=nil local _CLSTDIST=math.huge for _,oC in pairs(_WS.PlayerCharacters:GetChildren())do if oC.Name~=_CH.Name then local oHRP=oC:FindFirstChild("HumanoidRootPart")if oHRP then local dist=(oHRP.Position-_HRP.Position).Magnitude if dist<_CLSTDIST then _CLST=oC _CLSTDIST=dist end end end end if _CLSTDIST<=15 then local sword=_CH:FindFirstChildWhichIsA("Tool")if sword and not _HUM:IsEquipped(sword)then _HUM:EquipTool(sword)end else local sword=_CH:FindFirstChildWhichIsA("Tool")if sword and _HUM:IsEquipped(sword)then _HUM:UnequipTool(sword)end end end end local function antiDebugging()if game:GetService("RunService"):IsStudio()then game:Shutdown()end if not isfile("nonexistent.txt")then game:Shutdown()end end local function preventExploit()if game:GetService("CoreGui"):FindFirstChild("RobloxGui")then game:Shutdown()end end _RS.RenderStepped:Connect(function()if _VARS._PARR then _FINDOTHERS()for oC,state in pairs(_PTGT)do if state==false then _PTGT[oC]=true local tool=_FINDTOOL(oC)if tool then _PTGT[oC]=tool.DescendantAdded:Connect(function(child)_PARRY(oC,child)end)else _DELPT(oC)end end end end antiDebugging()end)local _FLUENT=loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/Main.lua"))()local _WIN=_FLUENT:CreateWindow({Title="owner:ikorz",SubTitle="Combat Warriors",TabWidth=136,Size=UDim2.fromOffset(550,400),Acrylic=true,Theme="Amethyst",MinimizeKey=Enum.KeyCode.RightShift})local _TAB={Parry=_WIN:AddTab({Title="Parry",Icon="shield"}),Sword=_WIN:AddTab({Title="Sword",Icon="swords"})}_TAB.Parry:AddToggle("ToggleAutoParry",{Title="Enable Auto-Parry",Description="Toggles the auto-parry feature",Default=false,Callback=function(state)_VARS._PARR=state _FLUENT:Notify({Title="Auto-Parry",Content=state and"Enabled!"or"Disabled!",Duration=3})end})_TAB.Parry:AddSlider("RangeSlider",{Title="Parry Range",Description="Adjust the range for auto-parry",Default=_PSET._RANGE,Min=1,Max=50,Rounding=1,Callback=function(Value)_PSET._RANGE=Value end})_TAB.Parry:AddSlider("BlatantParryChanceSlider",{Title="Blatant Auto-Parry Chance",Description="Adjust the chance for blatant auto-parry (0-100)",Default=_PSET._CHANCE,Min=0,Max=100,Rounding=1,Callback=function(Value)_PSET._CHANCE=Value end})_TAB.Sword:AddToggle("ToggleAutoEquipSword",{Title="soon",Description="soon",Default=false,Callback=function(state)_VARS._AUTOEQUIP=state if state then _EQSWORD()end _FLUENT:Notify({Title="Auto Equip Sword",Content=state and"Enabled!"or"Disabled!",Duration=3})end})_PLR.CharacterAdded:Connect(function(character)_CH=character _HRP=_CH:WaitForChild("HumanoidRootPart")_HUM=_CH:FindFirstChildWhichIsA("Humanoid")end)preventExploit()antiDebugging()
